{
  "name": "edoc",
  "version": "0.0.1",
  "description": "eDoc lines clients",
  "main": "main.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "start": "electron ./app",
    "dist:win": "electron-builder --windows --x64"
  },
  "author": "eDoc Saude <contato@edocsaude.com>",
  "build": {
    "appId": "com.github.edocsaude.edoc",
    "productName": "eDoc",
    "compression": "store",
    "publish": [
      {
        "provider": "github",
        "owner": "edocsaude",
        "repo": "lines_clients_test"
      }
    ],
    "nsis": {
      "perMachine": true
    }
  },
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-builder": "^19.13.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "electron-log": "^2.2.7",
    "electron-updater": "^2.4.5"
  }
}
